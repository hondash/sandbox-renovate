{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "labels": ["dependencies"],
  "additionalBranchPrefix": "{{parentDir}}-",
  "commitMessagePrefix": "[{{parentDir}}] ",
  "force": {
    "constraints": {
      "node": ">= 16.0.0"
    }
  },
  "includePaths": ["modules/sample-v16/package.json", "modules/sample-v14/package.json"]
}
